<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include
        android:id="@+id/title_id"
        layout="@layout/title_text"></include>


    <LinearLayout
        android:id="@+id/card_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="@dimen/dimen_5">

        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/num_linearlayout_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_marginLeft="@dimen/dimen_5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/item_num_title"
                        android:layout_width="@dimen/dimens_100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/itemnum"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />

                    <TextView
                        android:id="@+id/itemnum"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/item_num_title"
                        android:hint="@string/have_not_data"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />
                </LinearLayout>

                <View
                    android:id="@+id/view_1_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_line"
                    android:layout_below="@+id/num_linearlayout_id"
                    android:background="@drawable/line"></View>

                <LinearLayout
                    android:id="@+id/desc_linearlayout_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_below="@+id/view_1_id"
                    android:layout_marginLeft="@dimen/dimen_5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="@dimen/dimens_100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/description"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />

                    <TextView
                        android:id="@+id/description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/item_num_title"
                        android:hint="@string/have_not_data"
                        android:singleLine="true"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />
                </LinearLayout>


                <View
                    android:id="@+id/view_2_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_line"
                    android:layout_below="@+id/desc_linearlayout_id"
                    android:background="@drawable/line"></View>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_below="@+id/view_1_id"
                    android:layout_marginLeft="@dimen/dimen_5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/item_desc_title"
                        android:layout_width="@dimen/dimens_100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/spec"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />

                    <TextView
                        android:id="@+id/spec"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/item_num_title"
                        android:hint="@string/have_not_data"
                        android:singleLine="true"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />
                </LinearLayout>


                <View
                    android:id="@+id/view_3_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_line"
                    android:layout_below="@+id/desc_linearlayout_id"
                    android:background="@drawable/line"></View>

                <LinearLayout
                    android:id="@+id/in20_linearlayout_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_below="@+id/view_2_id"
                    android:layout_marginLeft="@dimen/dimen_5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/item_recorde_title"
                        android:layout_width="@dimen/dimens_100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/udspec"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />

                    <TextView
                        android:id="@+id/udspec"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/item_num_title"
                        android:hint="@string/have_not_data"
                        android:singleLine="true"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />
                </LinearLayout>

                <View
                    android:id="@+id/view_4_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_line"
                    android:layout_below="@+id/in20_linearlayout_id"
                    android:background="@drawable/line"></View>

                <LinearLayout
                    android:id="@+id/orderunit_linearlayout_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_below="@+id/view_3_id"
                    android:layout_marginLeft="@dimen/dimen_5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/item_orderunit_title"
                        android:layout_width="@dimen/dimens_100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/printqty"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />

                    <EditText
                        android:id="@+id/printqty"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/item_orderunit_title"
                        android:hint="@string/have_not_data"
                        android:inputType="number"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />
                </LinearLayout>

                <View
                    android:id="@+id/view_5_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_line"
                    android:layout_below="@+id/orderunit_linearlayout_id"
                    android:background="@drawable/line"></View>

                <LinearLayout
                    android:id="@+id/issueunit_linearlayout_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_below="@+id/view_4_id"
                    android:layout_marginLeft="@dimen/dimen_5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/item_issueunit_title"
                        android:layout_width="@dimen/dimens_100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/orderunit"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />

                    <TextView
                        android:id="@+id/orderunit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/item_num_title"
                        android:hint="@string/have_not_data"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />
                </LinearLayout>

                <View
                    android:id="@+id/view_6_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_line"
                    android:layout_below="@+id/issueunit_linearlayout_id"
                    android:background="@drawable/line"></View>

                <LinearLayout
                    android:id="@+id/enterby_linearlayout_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_below="@+id/issueunit_linearlayout_id"
                    android:layout_marginLeft="@dimen/dimen_5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/item_enterby_title"
                        android:layout_width="@dimen/dimens_100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/storeloc"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />

                    <TextView
                        android:id="@+id/storeloc"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/item_num_title"
                        android:hint="@string/have_not_data"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />
                </LinearLayout>

                <View
                    android:id="@+id/view_7_id"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_line"
                    android:layout_below="@+id/enterby_linearlayout_id"
                    android:background="@drawable/line"></View>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_below="@+id/issueunit_linearlayout_id"
                    android:layout_marginLeft="@dimen/dimen_5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="@dimen/dimens_100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/companyname"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />

                    <TextView
                        android:id="@+id/companyname"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/item_num_title"
                        android:hint="@string/have_not_data"
                        android:singleLine="true"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_line"
                    android:layout_below="@+id/enterby_linearlayout_id"
                    android:background="@drawable/line"></View>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_40dp"
                    android:layout_below="@+id/issueunit_linearlayout_id"
                    android:layout_marginLeft="@dimen/dimen_5"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="@dimen/dimens_100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/ponum"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp" />

                    <TextView
                        android:id="@+id/ponum"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@+id/item_orderunit_title"
                        android:hint="@string/have_not_data"
                        android:textColor="@color/body_text_1"
                        android:textSize="@dimen/dimens_16dp"/>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_line"
                    android:layout_below="@+id/enterby_linearlayout_id"
                    android:background="@drawable/line"></View>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="50dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/print"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/login_button"
                        android:text="@string/print"
                        android:textColor="@color/white" />


                </LinearLayout>
            </LinearLayout>

        </RelativeLayout>


    </LinearLayout>


</LinearLayout>
