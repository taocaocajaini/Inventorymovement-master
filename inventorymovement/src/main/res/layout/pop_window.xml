<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dip">

    <TextView
        android:id="@+id/item_generate_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="@string/generate_item_number"
        android:textSize="@dimen/dimens_16dp" />

    <View
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_line"
        android:layout_marginTop="5dip"
        android:background="@drawable/line"></View>

    <TextView
        android:id="@+id/item_work_flow_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dip"
        android:text="@string/work_flow"
        android:textSize="18dp"
        android:visibility="gone" />

</LinearLayout>
